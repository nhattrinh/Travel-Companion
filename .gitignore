*.xcworkspace/*
!*.xcworkspace/contents.xcworkspacedata

# Swift Package Manager
.swiftpm/
.build/
Packages/
/*.swiftpm

# CocoaPods
Pods/
*.podspec.json

# Carthage
Carthage/Build/

# Xcode user data
xcuserdata/

# iOS artifacts
*.ipa
*.dSYM.zip
*.dSYM

# Simulator/device state
*.xcuserstate

# ---- General OS / Editor ----
.DS_Store
Thumbs.db
desktop.ini
.idea/
.vscode/*
!.vscode/settings.json
!.vscode/extensions.json
!.vscode/launch.json

# Node (if any frontend tooling added later)
node_modules/
*.tsbuildinfo

# ---- Docker ----
# Ignore build cache layers if exported
*.tar

# ---- Secrets / Keys ----
*.key
*.pem
*.crt
secrets/

